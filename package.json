{
  "name": "whisper-diarization-server",
  "version": "1.0.0",
  "description": "API server to run the whisper-diarization Python scripts",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "morgan": "^1.10.0",
    "music-metadata": "^11.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "prettier": "^3.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
